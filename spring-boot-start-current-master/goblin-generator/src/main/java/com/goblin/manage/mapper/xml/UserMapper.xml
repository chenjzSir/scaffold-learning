        <result column="update_time" property="updateTime" />
        <result column="is_enabled" property="enabled" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, username, password, password_salt, nick_name, real_name, email, phone, user_image_url, last_password_reset_date, create_user_id, update_user_id, create_time, update_time, is_enabled, remark
